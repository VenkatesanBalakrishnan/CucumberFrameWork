package practiceAtOffice;

public class LoopsStar {
	
	
	
	public void printStar()
	{
		for(int col =1; col<=5;col++)
		{
			for(int row =1; row<=5;row++)
			{
				System.out.print("*");
			}
			System.err.println();
		}
	}
	public static void main(String[] args) {
		LoopsStar l = new LoopsStar();
		l.printStar();
	}
	

}
