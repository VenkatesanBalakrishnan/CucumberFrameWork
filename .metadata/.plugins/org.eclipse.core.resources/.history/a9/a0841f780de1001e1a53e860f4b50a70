package practice;

public class StringDemo {
	
	
	String s1=("Oranium Tech");
	String s2=("Java Programing");
	String s3 =("Java Programing");
	String s5=("  venkat  ");
	String s4= ("No 19, Krishna Street Irumbuliyur Chennai-600045");
	
	public void printString()
	{
		System.out.println(s1);
		System.out.println(s1.hashCode());
		System.out.println(s2.hashCode());
		System.out.println(s3.hashCode());
		
		System.out.println("String pre defined methods");
		
		System.out.println(s1.hashCode());//adress
		System.out.println(s1.toUpperCase());//lower to uppercase
		System.out.println(s1.toLowerCase()); // upper to lowercase
		System.out.println(s1.isEmpty());// check if it's empty
		System.out.println(s1.equals("Oranium Tech"));//check given word are equal
		System.out.println(s1.equalsIgnoreCase("ORANIUM TECH")); // check given word are equal and ignore case
		System.out.println(s1.startsWith("Oran")); // check the starting char are same
		System.out.println(s1.endsWith("Tech"));//  check the starting char are same
		System.out.println(s5.trim());// trim the empty spaces in start and end area
		System.out.println(s1.length());
		System.out.println(s1.charAt(4));
		System.out.println(s1.indexOf("i"));
		System.out.println(s2.lastIndexOf("a"));
		System.out.println(s1.contains("Tech"));
		System.out.println(s2.concat("Great"));
		
	}
	
	public static void main(String[] args) {
		
		StringDemo s = new StringDemo();
		s.printString();
	}

}
