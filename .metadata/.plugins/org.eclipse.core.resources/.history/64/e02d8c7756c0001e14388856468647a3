package practiceAtOffice;

public class OperatorDemos {
	
	int var1=12;
	int var2=21;
	
	public void arithmeticOps()
	{
		System.out.println(var1+var2);
		System.out.println(var1-var2);
		System.out.println(var1/var2);
		System.out.println(var1*var2);
		System.out.println(var1%var2);
	}
	
	public static void main(String[]args)
	{
		OperatorDemos a = new OperatorDemos();
		a.arithmeticOps();
	}

}
