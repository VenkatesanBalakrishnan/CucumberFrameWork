package polymorphism;

public class CalculatorDemo {
	
	int var1=10;
	int var2=3;
	
	public void addNumbers()
	{
		System.out.println(("Addition is ") + (var1+var2));
	}
	public void addNumbers(int a, int b)
	{
		System.out.println(("Two INT Paara is ") + (a+b));
	}
	
	public void addNumbers(double a, int b)
	{
		System.out.println(("one double and one int paara is ") + (a+b));
	}
	public void addNumbers(int a, double b)
	{
		System.out.println(("one int and one double paara is ") + (a+b));
	}
	
	public static void main(String[] args) {
		CalculatorDemo c = new CalculatorDemo();
		c.addNumbers();
		c.addNumbers(400, 100);
		c.addNumbers(3.5, 100);
		c.addNumbers(200, 2.44);
	}

}
